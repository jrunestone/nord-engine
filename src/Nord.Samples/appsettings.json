{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },

    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    },
    
    "Nord": {
        "Name": "nord",
        "Resolution": "1024x768",
        "Windowed": true,
        "AssetRootPath": "assets"
    }
}